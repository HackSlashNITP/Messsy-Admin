<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hackslash.messsyadmin">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MesssyAdmin">
        <activity android:name=".MessMember.EditProfileMessMember"></activity>
        <activity android:name=".Admin.EditProfileAdmin" />
        <activity android:name=".ContactUs" />
        <activity android:name=".fragmentcontainer">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity_complaintBox" />
        <activity android:name=".activity_conversations" />
        <activity android:name=".activity_createNewNotice" />
        <activity android:name=".activity_studentTrack" />
        <activity android:name=".activity_absenteesList" />
        <activity android:name=".activity_ReportAnIssueUI" />
        <activity android:name=".Admin.AdminUI" />
        <activity android:name=".MessMember.MessMemberUI" />
        <activity android:name=".activity_Login" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>